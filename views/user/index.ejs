<h1>Users</h1>

<table>

  <thead>
    <tr>
      <td>Name</td>
      <td>Email</td>
      <td>Phone</td>
      <td>Role</td>
      <td></td>
    </tr>
  </thead>

  <tbody>
    <% users.forEach(function( user ) { %>
      <tr>
        <td><%= user.name %></td>
        <td><%= user.email %></td>
        <td><%= user.phoneNumber %></td>
        <td><%= user.role %></td>
        <td>
          <a href="/user/edit/<%= user.id %>">edit</a>
          <a href="/user/destroy/<%= user.id %>">delete</a>
        </td>
      </tr>
    <% }) %>
  </tbody>

</table>
