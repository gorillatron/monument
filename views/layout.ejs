<!DOCTYPE html>
<html>
  <head>

    <title>Monument, Norwegian Techno, Culture and Community.</title>
    <meta property="og:title"content="Monument, Norwegian Techno, Culture and Community." />

    <meta name="description" property="og:description" content="Monument is a techno podcast and community based in Oslo Norway. We are promoting and cultivating quality underground techno. Feel free to join us on this mystical journey." />
    <meta name="keywords" content="techno, culture, norway, oslo, community, love, electronic, music, podcast">

    <meta name="image" property="og:image" content="http://monument.herokuapp.com/images/2_1.png" />
    <meta property="og:image:secure_url" content="https://monument.herokuapp.com/images/2_1.png" />

    <meta name="robots" content="index,follow">

    <!-- fav icon -->
    <link rel="icon" type="image/ico" href="/images/logo.ico"/>

    <link rel="stylesheet" href="/styles/index.css">

    <!-- Viewport mobile tag for sensible mobile support -->
    <meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1">

    <% if(currentPage.name == 'podcasts') { %>
      
      <meta property="og:type"        content="music.playlist" />
      <meta property="og:url"         content="https://soundcloud.com/monument-podcast" />
      <meta property="og:title"       content="Monument" />

      <% podcasts.forEach(function(podcast) { %>

        <meta property="music:song"     content="<%= podcast.soundcloudTrack.title %>" />
        <meta property="music:song:url" content="<%= podcast.soundcloudTrack.permalink_url %>" />

      <% }) %>


    <% }%>

  </head>

  <body>

    <div class="container">

      <%- partial ('main-header.ejs') %>

      <% if(typeof showSubscribeBanner !== 'undefined' && showSubscribeBanner){ %>
        <%- partial ('subscribebanner.ejs') %>
      <% } %>


      <div id="content-wrapper">
        <%- body %>
      </div>

    </div>

    <%- partial ('analytics_script.ejs') %>

  </body>
</html>
